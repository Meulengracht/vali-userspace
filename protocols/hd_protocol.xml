<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<root>
    <types>
        <type name="uint32_t" header="stdint.h" />
    </types>
    <protocols namespace="hd">
        <protocol name="core" id="0x20">
            <functions>
                <function name="notification">
                    <request>
                        <param name="serial" type="uint32_t"/>
                    </request>
                </function>
            </functions>
            
            <events>
                <event name="error">
                    <param name="object_id" type="uint32_t"/>
                    <param name="error_id" type="int"/>
                    <param name="error_description" type="string" />
                </event>
                <event name="notification_response">
                    <param name="serial" type="uint32_t"/>
                </event>
            </events>
        </protocol>
    </protocols>
</root>
